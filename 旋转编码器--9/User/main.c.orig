#include "stm32f10x.h"                  // Device header
#include "Delay.h"
#include "OLED.h"
#include "Encoder.h"
int main(void)
{
    OLED_Init();
    Encoder_Init();
    int16_t Num = 0;

    while(3) {};
    while(3) {};
    while(3) {};
    OLED_ShowString(1, 1, "Num:");
    while(1)
    {
        Num += Encoder_GET();
        OLED_ShowSignedNum(1, 5, Num, 5);
    }
}
